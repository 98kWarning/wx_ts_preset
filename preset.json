{
  "useTaobaoRegistry": true,
  "packageManager": "yarn",
  "useConfigFiles": true,
  "router": false,
  "vuex": false,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": false,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "base",
      "lintOn": [
        "save"
      ]
    },
    "configs": {
      "eslintConfig": {
        "rules": {
          "no-alert": 0
        }
      }
    }
  }
}
